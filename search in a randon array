
public class Main {

    public static void main(String[] args) {

        int arr[] = {4,5,6,7,0,1,2};
        int n = arr.length;
        int target =3;
        int j=0;
        for (int i=0;i<n;i++ ){
            if (arr[i]==target){
                j=i;
            }else j=-1;
        }
        System.out.println(j);



}

}
