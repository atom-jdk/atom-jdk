public class Main {

    public static void main(String[] args) {
        int array[]={2,7,11,15};
        int target =13;
        for (int i=0;i< array.length;i++){
            for (int j=0;j< array.length;j++){
                if (array[i]+array[j]==target){
                    System.out.println(target+" can be obtained by adding "+array[i]+" and "+array[j]);
                }
            }
        }

    }
}
